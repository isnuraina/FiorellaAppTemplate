@model IEnumerable<AppUser>
<div class="container">
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">User Table</h4>
                    <a class="btn btn-outline-success" asp-action="create">Create</a>
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr>

                                    <th>FullName</th>
                                    <th>UserName</th>
                                    <th>Email</th>
                                    <th>Setting</th>                                   
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var user in Model)
                                {
                                    <tr>
                                     

                                        <td>@user.FullName</td>
                                        <td>@user.UserName</td>
                                        <td>@user.Email</td>
                                    
                                        <td>
                                            <a class="btn btn-outline-primary" asp-action="detail" asp-route-id="@user.Id">Detail</a>
                                            <a class="btn btn-outline-success" asp-action="update" asp-route-id="@user.Id">Update</a>
                                            <a class="btn btn-outline-danger" asp-action="delete" asp-route-id="@user.Id">Delete</a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>